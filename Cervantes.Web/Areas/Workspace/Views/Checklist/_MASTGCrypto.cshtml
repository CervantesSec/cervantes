                        @Html.DropDownListFor(model => model.Crypto.CryptoStatus04, Html.GetEnumSelectList<Cervantes.CORE.MASTGStatus>(), new {@class = "form-control"})
                    </td>
                    <td><textarea asp-for="Crypto.CryptoNote04" class="form-control" rows="4"></textarea></td>
                </tr>
